pipeline {
	agent any
		stages {
			stage('Clean stage') {
				steps {
					dir("MiscTopicsDemo") {
						bat '"C:/apache-maven-3.8.5/bin/mvn" clean'
					}
				}
			}
			stage('Compile stage') {
				steps {
					dir("MiscTopicsDemo") {
						bat '"C:/apache-maven-3.8.5/bin/mvn" compile'
					}
				}
			}
			stage('Install stage') {
				steps {
					dir("MiscTopicsDemo") {
						bat '"C:/apache-maven-3.8.5/bin/mvn" install'
					}
				}
			}
		}
}
